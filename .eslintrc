{
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "eqeqeq": [
      2
    ]
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "chrome": false
  }
}
